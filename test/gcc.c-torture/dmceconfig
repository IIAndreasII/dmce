# DMCE install path
DMCE_EXEC_PATH:

# Working directory root
DMCE_WORK_PATH:/tmp/$USER/dmce

# config files path. (dmce.exclude, dmce.include etc.)
DMCE_CONFIG_PATH:

# Command line lookup hook (builds list of individual compile commands per file if not enough with the default ones)
DMCE_CMD_LOOKUP_HOOK:

# Default compiler command line for c files
DMCE_DEFAULT_C_COMMAND_LINE:gcc -isystem $DMCE_WORK_PATH/inc -I$DMCE_WORK_PATH/inc

# Default compiler command line for cpp files
DMCE_DEFAULT_CPP_COMMAND_LINE:gcc -std=c++11 /usr/include -isystem $DMCE_WORK_PATH/inc -I$DMCE_WORK_PATH/inc

# Default compiler command line for h files
DMCE_DEFAULT_H_COMMAND_LINE:gcc -isystem $DMCE_WORK_PATH/inc -I$DMCE_WORK_PATH/inc

# Probe definition c file
DMCE_PROBE_SOURCE:

# Probe prolog c file
DMCE_PROBE_PROLOG:

# Log files
DMCE_LOG_FILES:$DMCE_WORK_PATH

# git diff algorithm
DMCE_GIT_DIFF_ALGORITHM:histogram
