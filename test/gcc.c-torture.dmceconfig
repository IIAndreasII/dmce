# DMCE install path
DMCE_EXEC_PATH:/tmp/$USER/dmce/test/gcc.c-torture/dmce

# Working directory root
DMCE_WORK_PATH:/tmp/$USER/dmce

# config files path. (dmce.exclude, dmce.include etc.)
DMCE_CONFIG_PATH:/tmp/$USER/dmce/test/gcc.c-torture/dmce

# Command line lookup hook (builds list of individual compile commands per file if not enough with the default ones)
DMCE_CMD_LOOKUP_HOOK:/tmp/$USER/dmce/test/gcc.c-torture/dmce/cmdlookuphook.sh

# Default compiler command line for c files
DMCE_DEFAULT_C_COMMAND_LINE:gcc -isystem /tmp/$USER/dmce/inc -I/tmp/$USER/dmce/inc

# Default compiler command line for cpp files
DMCE_DEFAULT_CPP_COMMAND_LINE:gcc -std=c++11 /usr/include -isystem /tmp/$USER/dmce/inc -I/tmp/$USER/dmce/inc

# Probe definition c file
DMCE_PROBE_SOURCE:/tmp/$USER/dmce/test/gcc.c-torture/dmce/dmce-probe-monolith.c

# Log files
DMCE_LOG_FILES:/tmp/$USER/dmce
